{
  "name": "@bare/react-native-video-screenshot-example",
  "version": "1.0.0",
  "private": true,
  "description": "Example React Native app demonstrating react-native-video-screenshot-plugin",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start": "react-native start",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "clean": "cd ios && xcodebuild clean && cd ../android && ./gradlew clean && cd ..",
    "pods": "cd ios && pod install && cd .."
  },
  "dependencies": {
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-video": "^6.4.0",
    "react-native-video-screenshot-plugin": "file:../../",
    "react-native-fs": "^2.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/eslint-config": "0.79.2",
    "@react-native/metro-config": "0.79.2",
    "@react-native/typescript-config": "0.79.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "react-native",
    "react-native-video",
    "screenshot",
    "example",
    "demo"
  ]
}
